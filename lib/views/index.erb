<script id="pwTpl" type="text/template"> 
         <% flash_types.select{ |kind| flash.has?(kind) }.each do |kind| %>
      <div class="flash <%= kind %>">
        <%= flash[kind] %>
      </div>
      <% end %> 
        <form action="/reset" method="post" >
        <%= Rack::Csrf.csrf_tag(env) %>
          <fieldset>
            <legend>Reset password</legend> 

             <div class="row">
              <div class="one whole padded">
                <label for="email">Email Address</label>
                <input type="email" name="user[email]" placeholder="Your Email" id="email" />
              </div>
            </div>
            <div class="row">
              <div class="one whole padded">
                <button type="submit">Submit</button> 
              </div>
            </div>                       
          </fieldset>
         
        </form>
</script>

      <script id="signupTpl" type="text/template">    
         <% flash_types.select{ |kind| flash.has?(kind) }.each do |kind| %>
<div class="flash <%= kind %>">
  <%= flash[kind] %>
</div>
<% end %> 
        <form action="/signup" method="post" >
        <%= Rack::Csrf.csrf_tag(env) %>
          <fieldset>
            <legend>Register</legend> 

             <div class="row" id="msv">
              <div class="one whole padded">
                <label for="msv">MSV</label>
                <input type="text" name="user[msv]" placeholder="Your MSV" id="msv" />
              </div>
            </div>

            <div class="row">
              <div class="one whole padded">
                <label for="email">Email Address</label>
                <input type="email" name="user[email]" placeholder="Your Email" id="email" />
              </div>
            </div>
            <div class="row">
              <div class="one whole padded">
                <label for="password">Password</label>
                <input type="password" name="user[password]" placeholder="Your password" id="password" />
              </div>
            </div>
            <div class="row">
              <div class="one whole padded">
                <label for="password2">Password</label>
                <input type="password" name="user[password2]" placeholder="Confirm your password" id="password2" />
              </div>
            </div>
            <div class="row">
              <div class="one whole padded">
                <label for="fullname">Full Name</label>
                <input type="text" name="user[hovaten]" placeholder="Your full name" id="fullname" />
              </div>
            </div>
            <div class="row">
             <div class="one whole padded">
                <select class="unselected">
                  <option selected="" disabled="" value="" name="user[gioitinh]">Gioi tinh</option>
                  <option value="1">Nam</option>
                  <option value="0">Nu</option>
                </select>
             </div>
            </div>
            <div class="row">
              <div class="one whole padded">
                  <label for="datepicker">Birth Date</label>
                  <input type="text" id="datepicker" placeholder="dd/mm/yyyy" name="user[ngaysinh]" /></p>
                </div>
              </div>            
            <div class="row">
              <div class="one whole padded">
                <label for="address">Address</label>
                <input type="text" name="user[diachi]" placeholder="Street Address" id="address">
              </div>
            </div>                        
           
           <div class="row">
              <div class="one whole padded">
                <label for="phone">Phone</label>
                <input type="text" name="user[sodienthoai]" placeholder="So dien thoai" id="phone">
              </div>
            </div> 
          
            <div class="row">
              <div class="one whole padded">
                <button type="submit">Submit</button> 
              </div>
            </div>                       
          </fieldset>
         
        </form>
        </script>
